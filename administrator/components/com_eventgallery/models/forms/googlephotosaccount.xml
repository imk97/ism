<?xml version="1.0" encoding="utf-8"?>
<form name="googlephotosaccount">

  


    <fieldset name="general" label="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_LABEL" description="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_DESC">
    	<field name="id" type="hidden" default="0" label="JGLOBAL_FIELD_ID_LABEL"
			readonly="true" class="readonly"
			description="JGLOBAL_FIELD_ID_DESC"/>
			
        <field name="name"
               type="text"
               required="true"
               inputtype="text"
               label="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_NAME"
               description="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_NAME_DESCRIPTION"
                />

		<field name="description"
               type="text"
               inputtype="text"
               label="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_DESCRIPTION"
               description="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_DESCRIPTION_DESCRIPTION"
                />

        <field name="clientid"
               type="text"
               required="true"
               inputtype="text"
               label="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_CLIENTID"
               description="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_CLIENTID_DESCRIPTION"
        />

        <field name="secret"
               type="text"
               required="true"
               inputtype="text"
               label="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_SECRET"
               description="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_SECRET_DESCRIPTION"
        />

        <field 	name="refreshtoken"
                  type="googlephotosrefrehtoken"
                  default=""
                  label="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_REFRESHTOKEN"
                  description="COM_EVENTGALLERY_GOOGLEPHOTOSACCOUNT_REFRESHTOKEN_DESCRIPTION" />

        
         
    </fieldset>
</form>